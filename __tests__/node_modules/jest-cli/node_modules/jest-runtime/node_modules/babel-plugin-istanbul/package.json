{
  "name": "babel-plugin-istanbul",
  "version": "1.1.0",
  "author": {
    "name": "Thai Pangsakulyanont @dtinth"
  },
  "license": "BSD-3-Clause",
  "description": "A babel plugin that adds istanbul instrumentation to ES6 code",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "find-up": "^1.1.2",
    "istanbul-lib-instrument": "^1.1.0-alpha.4",
    "test-exclude": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
